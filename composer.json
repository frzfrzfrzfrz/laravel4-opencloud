{
    "name": "unm/laravel4-opencloud",
    "description": "A laravel 4.1 service provider package that wraps the Rackspace/Openstack PHP SDK",
    "keywords"   : ["rackspace","openstack","opencloud","nova","swift","php","laravel"],
    "homepage"   : "https://github.com/undernewmanagement/laravel4-opencloud",
    "license"    : "MIT",
    "authors": [
        {
            "name"     : "Under New Management",
            "homepage" : "http://github.com/undernewmanagement",
            "role"     : "developer"
        }
    ],
    "support" : {
        "issues" : "https://github.com/undernewmanagement/laravel4-opencloud/issues"
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": ">=4.1.0",
        "rackspace/php-opencloud": "~1.11.0"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Unm\\Laravel\\OpenCloud\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
